            id="image"
            layout_width="MATCH_PARENT"
            layout_height="WRAP_CONTENT"
            margin_left="2"
            margin_right="2"
            margin_top="2"
            margin_bottom="2"/>
    <ImageView id="selected" layout_width="0" layout_height="0" drawable="@drawables/selected_json" layout_fill="true"/>
</FrameLayout>