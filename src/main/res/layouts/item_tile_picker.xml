<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:ge="http://www.w3.org/2001/XMLSchema-instance"
              ge:noNamespaceSchemaLocation="layout_schema.xsd"
              layout_width="MATCH_PARENT"
              layout_height="WRAP_CONTENT">
